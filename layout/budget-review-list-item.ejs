<tr>
  <td style="width: 80%" class="py-3 px-4">
    <div>
      <p class="fs-8 mb-2">
        <%= details %>
        <span class="font-Nunito fs-10"><%= spentAmount / spentTotal * 100 %>%</span>
      </p>

      <div
        class="progress"
        role="progressbar"
        aria-label="progress bar"
        aria-valuenow="<%= spentAmount / spentTotal * 100 %>%"
        aria-valuemin="0"
        aria-valuemax="100"
        style="height: 8px">
        <div class="progress-bar" style="width: <%= spentAmount / spentTotal * 100 %>%; height: 8px"></div>
      </div>
    </div>
  </td>
  <td class="text-end py-3 px-4">
    <p class="spent-amount font-Nunito fw-normal">$<%= spentAmount.toLocaleString('en-IN') %></p>
  </td>
</tr>
